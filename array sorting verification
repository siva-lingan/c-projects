#include<stdio.h>
int main(){
    int a[5] = {1,2,3,4,5};
    int n = sizeof(a)/sizeof(a[0]);
    int flag = 0;
    for(int i =0;i<n-1;i++){
        if(a[i]>a[i+1]){
            flag = 1;
        }
        
    }
    if(flag==0){
        printf("array is sorted");
    }
    else{
        printf("array is not sorted");
    }
    return 0;
        
}
